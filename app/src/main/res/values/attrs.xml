<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NavigationView">
        <attr name="nav_title" format="string" />
        <attr name="nav_show_back" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RoundAngleImageView">
        <attr name="img_radius" format="dimension" />
        <attr name="img_left_top_radius" format="dimension" />
        <attr name="img_right_top_radius" format="dimension" />
        <attr name="img_right_bottom_radius" format="dimension" />
        <attr name="img_left_bottom_radius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SettingItemView">
        <attr name="setting_item_icon" format="reference" />
        <attr name="setting_item_title" format="string" />
    </declare-styleable>


    <declare-styleable name="PictureLongScaleImageView">
        <attr name="src" format="reference" />
        <attr name="assetName" format="string" />
        <attr name="panEnabled" format="boolean" />
        <attr name="zoomEnabled" format="boolean" />
        <attr name="quickScaleEnabled" format="boolean" />
        <attr name="tileBackgroundColor" format="color" />
    </declare-styleable>

    <declare-styleable name="PictureMediumBoldTextView">
        <attr name="stroke_Width" format="float" />
    </declare-styleable>

    <declare-styleable name="PictureRoundCornerRelativeLayout">
        <attr name="corners" format="dimension" />
        <attr name="topNormal" format="boolean" />
        <attr name="bottomNormal" format="boolean" />
    </declare-styleable>
</resources>